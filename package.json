{
  "name": "emotions",
  "version": "0.1.0",
  "scripts": {
    "watch": "BS_WATCH_CLEAR=true bsb -make-world -w",
    "start": "parcel src/index.html",
    "prestart": "yarn run clean:bs && yarn run build:bs",
    "build": "parcel build src/index.html",
    "prebuild": "yarn run clean:dist && yarn run clean:bs && yarn run build:bs",
    "build:bs": "bsb -make-world",
    "clean:bs": "bsb -clean-world",
    "clean:dist": "rm -rf dist"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ahrefs/bs-emotion-ppx": "2.0.0",
    "@babel/core": "^7.0.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.4",
    "@minima.app/re-css": "1.0.1",
    "parcel-bundler": "1.12.4",
    "re-classnames": "4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reason-react": "^0.9.1",
    "bs-platform": "^8.4.2"

  }
}
